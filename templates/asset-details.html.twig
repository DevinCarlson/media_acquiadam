<div class="asset-preview">
  <img src="{{ asset_preview }}" />
    <br />
    <a target="_blank" href="{{ asset_link }}">Open in Acquia DAM</a>
</div>

<div class="asset-data">
    <table>
        <tr>
            <td><strong>Attribute</strong></td>
            <td><strong>Value</strong></td>
        </tr>
        {% for attribute, value in asset_data %}
        <tr>
            <td>{{ attribute }}</td>
            <td>{{ value }}</td>
        </tr>
        {% endfor %}
    </table>
</div>
